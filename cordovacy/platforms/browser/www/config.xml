<?xml version='1.0' encoding='utf-8'?>
<widget id="com.josephstahl.cordovacy" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaCy</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="joseph@josephstahl.com" href="http://blog.js.cytoscape.org">
        Joseph Stahl for GSoC 2016
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
